func int64 ackermann(int64 m, int64 n)
{
    int64 ans;
    if(m == 0)
    {
        ans = n+1;
    }
    else if(n == 0)
    {
        ans = ackermann(m-1,1);
    }
    else
    {
        ans = ackermann(m-1, ackermann(m,n-1));
    }
    return ans;
}

func void main()
{
    int64 x = ackermann(3,4);
}